 {% if paramQ %}
  <div class="row">
    <div class="col-lg-6">
      <h1>Your Search <span>"{{ paramQ }}"</span></h1>
    </div>
    <div class="col-lg-6 d-flex justify-content-end ">
      <h5>{{ paginator.getStart() + 1 }}-{{ paginator.currentTotal() }} of about {{ paginator.total }} Results</h5>
    </div>
  </div>
{% endif %}
<div class="row">
  <div class="col-lg-12">
    <div class="form-group has-search">
      <span class="fa fa-search form-control-feedback"></span>
      <input type="text" class="form-control searchpage-bar" name="q" value="{{ paramQ }}" placeholder="Dubai Expo 2021">
      <button type="Search" class="btn btn-primary searchpage-btn">Search</button>
    </div>
  </div>
</div>
