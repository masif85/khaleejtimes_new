<!-- Search Modal -->
<div class="search-main-wrapper-nf modal fade" id="searchModal" tabindex="-1" aria-labelledby="searchModalLabel" aria-hidden="true">
  <div class="modal-dialog search-wrapper-nf">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id=""> <a href="/" class="standard-logo"><img src="{{ get_stylesheet_directory_uri() }}/assets/images/khaleej-times-logo.svg" width="206" height="36" alt="khaleejtimes"></a></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body d-flex flex-column h-100 justify-content-center align-items-center">
        <div class="search-wrap-inner">
          <h2>Type your keywords</h2>
          <form name="searchform" id="searchform" action="/search">
            <input type="text" id="search" class="search-field-nf" placeholder="Enter keywords here.." name="q">
            <button type="submit" class="search-btn">Search</button>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>

