<div class="row no-gutters">
  <div class="col-lg-12">
    <div class="draft-rates-main-wrapper-nf">
      <div class="draft-rates-rt-wrap">
        <table class="table table-hover">
          <thead>
            {% for tr in thead ?? [] %}
              <tr>
                {% for value in tr.th %}
                  <th scope="col">{{ value }}</th>
                {% endfor %}
              </tr>
            {% endfor %}
          </thead>
          <tbody>
            {% for tr in tbody ?? [] %}
              <tr>
                {% for value in tr.td %}
                  {% if loop.first %}
                    <th scope="row">{{ value }}</th>
                  {% else %}
                    <td >{{ value }}</td>
                  {% endif %}
                {% endfor %}
              </tr>
            {% endfor %}
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
