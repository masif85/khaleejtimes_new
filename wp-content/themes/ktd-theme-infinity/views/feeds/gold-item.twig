{% if content %}
{% if content.object["@attributes"].type == 'x-im/table' %}
<Parent date="{{ "now"|date("m/d/Y") }}">
    {% for tr in content.object.data.tbody.tr %}

        {% if loop.index == 1 %}
        <goldpricett>
            {% elseif loop.index == 2 %}
            <goldprice24>
                {% elseif loop.index == 3 %}
                <goldprice22>
                    {% elseif loop.index == 4 %}
                    <goldprice21>
                        {% else %}
                        <goldprice18>
                            {% endif %}
    <price goldmorning="{{ tr.td[1]|trim }}" goldnoon="{{ tr.td[2]|trim }}" goldyesterday="{{ tr.td[3]|trim }}"/>
        {% if loop.index == 1 %}
        </goldpricett>
            {% elseif loop.index == 2 %}
            </goldprice24>
                {% elseif loop.index == 3 %}
                </goldprice22>
                    {% elseif loop.index == 4 %}
                    </goldprice21>
                        {% else %}
                        </goldprice18>
                            {% endif %}
    {% endfor %}
</Parent>
{% endif %}
{% endif %}









