                  <script>
                      googletag.cmd.push(function() { googletag.display('div-gpt-ad-1613548422640-0'); });
                  </script>
                </div> -->
                <!-- Tag ID: khaleejtimes_Responsive-Section-Leaderboard-3 -->
                <div align="center" data-freestar-ad="__320x100 __728x90" id="khaleejtimes_Responsive-Section-Leaderboard-3">
                  <script data-cfasync="false" type="text/javascript">
                    freestar.config.enabled_slots.push({ placementName: "khaleejtimes_Responsive-Section-Leaderboard-3", slotId: "khaleejtimes_Responsive-Section-Leaderboard-3" });
                  </script>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>

      <aside class="col-sm-12 col-md-12 col-lg-4">
        <div class="article-right-sidebar">
          <!-- Right side -->
          {{ render_sidebar("subsection-sidebar") }}
        </div>
      </aside>
    </div>
  </div>  



