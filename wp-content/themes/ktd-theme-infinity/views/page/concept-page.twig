                              googletag.cmd.push(function() { googletag.display('div-gpt-ad-1613547811534-0'); });
                          </script>
                        </div> -->
                        <!-- Tag ID: khaleejtimes_Responsive-Section-Leaderboard-2 -->
                        <div align="center" data-freestar-ad="__320x100 __728x90" id="khaleejtimes_Responsive-Section-Leaderboard-2">
                          <script data-cfasync="false" type="text/javascript">
                            freestar.config.enabled_slots.push({ placementName: "khaleejtimes_Responsive-Section-Leaderboard-2", slotId: "khaleejtimes_Responsive-Section-Leaderboard-2" });
                          </script>
                        </div>
                      </div>
                    {% endif %}
                  {% endif %}
                {% endfor %}
              </div>
            </div>
          </div>

          <div class="row">
            <div class="col-lg-12">
              <div class="pagination-inline ">
                {{ paginator.render() }}
              </div>
            </div>
          </div>
        {% endif %}

        <div class="module-5 billboard-banner">
          <div class="container">
            <div class="row">
              <div class="col-auto mx-auto">
                <!-- /78059622/Responsive-Section-Leaderboard-3 -->
