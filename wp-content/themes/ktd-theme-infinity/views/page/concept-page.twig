  <div class="container">
    <div class="row">
      <div class="col-lg-8">
        <div class="article-wrapper subsection-lead">
          <div class="row">
            <div class="col-lg-12">
              {% if articles is not empty %}
                {% for article in articles %}
                  {% if loop.first %}
                    {{ render_partial('@base/teasers/standard-teaser.twig', article) }}
                  {% endif %}
                {% endfor %}
              {% else %}
                <h1>Sorry, we didn't find what you were searching for.</h1>
              {% endif %}
            </div>
          </div>
        </div>

        {% if articles is not empty %}
          <div class="subsection">
            <div class="row no-gutters">
              <div class="section-header col">
                <h1 class="section-title text-uppercase font-weight-bold bordered-left">MORE FROM {{ currentPage.post_title }}</h1>
              </div>
            </div>
            <div class="row">
              <div class="col-lg-12">
                {% for article in articles %}
                  {% if loop.first %}
                    <!-- Skip first -->
                  {% else %}
                    {{ render_partial('@base/teasers/standard-subsection-teaser.twig', article) }}
						  {%if loop.index==4%}
						<!-- googleoff: index -->
						<div id="adsolut-listing"></div>
						<!-- googleon: index -->
					{%endif%}
                    {% if loop.index == 5 %}
                      <div class="mpu-ad-unit ad-unit">
