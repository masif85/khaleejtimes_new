 <span class="mweather">
 <script>
  jQuery.ajax({url: "https://api.khaleejtimes.com/home/get_weather",
   type: "GET",
   dataType: "json",
   success: function (result) { 
    jQuery(".wicon").attr("src","/images/icons/"+result.type);
    jQuery({ Counter: 0 }).animate({ Counter: result.temperature }, {
    duration: 1000,
    easing: 'swing',
    step: function (now) {

      jQuery(".cweather").text(Math.ceil(now));
    }
    }); 
   jQuery(".mweather").remove();}});
 </script>
 </span>
