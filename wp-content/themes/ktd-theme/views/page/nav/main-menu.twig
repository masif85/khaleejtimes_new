      <img src="/images/icons/weather-sun.svg" class="wicon" alt="ktweather icon" width="23" height="17"  onerror="console.log(this.src); this.src ='/images/icons/weather-sun.svg'; console.log(this.src);"><span class="cweather" style="display: inline-block;min-width: 18px;">0{#get_weather()#}</span>°C </span> </p>
