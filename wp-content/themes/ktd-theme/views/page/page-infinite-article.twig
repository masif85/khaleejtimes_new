{% set replaced_link = get_permalink()|replace({'https://www.khaleejtimes.com' : ""}) %}
<script>
var event_timestamp_utc = dNow.getUTCFullYear() + '-' + ('0' + (dNow.getUTCMonth() + 1)).slice(-2) + '-' + ('0' + dNow.getUTCDate()).slice(-2) + ' ' + ('0' + dNow.getUTCHours()).slice(-2) + ':' + ('0' + dNow.getUTCMinutes()).slice(-2) + ':' + ('0' + dNow.getUTCSeconds()).slice(-2);
/*
var tealium_visitor_id = /(v_id:(\w+))/.exec(window.document.cookie)[2];
var match = /_ga=(GA1\.2\.\d+\.\d+)/.exec(window.document.cookie);
var googleid = match ? match[1] : null;
*/
var newsKeywords = jQuery('meta[name="news_keywords"]').attr('content');
newsKeywords = newsKeywords || null;


console.log('infinite-scroll-utag-trigger');
 var utag_data = {
  "article_authors"      : "{% if authors %}{% for author in authors %}{{ author.name }}{% if loop.last == false %}, {% endif %}{% endfor %}{% elseif author_byline %}{{ author_byline }}{% else %}None{% endif %}",
  "article_id"           : "{{ uuid }}",
  "article_publish_date" : "{{ pubdate|date('Y-m-d') }}T{{ pubdate|date('H:i:s') }}Z",
  "article_title"        : "{{ teaser_headline|replace({"\n":'', "\r":''}) }}",
  "article_type"         : "{% if contentsubtype == 'VIDEO' %}VIDEO{% elseif contentsubtype == 'GALLERY' %}GALLERY{% else %}ARTICLE{% endif %}",
  "event_type"           : "page_view",
  "is_article_premium"   : "0",
  "page_path"            : "{{ replaced_link }}",
  "page_url"             : "{{ get_permalink() }}",
  "page_category"        : "{% for items in sections|sort|reverse|slice(0, 1) %}{{ items.name }}{% endfor %}",
  "page_country"         : "eg",
  "page_currency_code"   : "AED",
  "page_lang"            : "English",
  "page_section"         : "{% for items in sections|sort|reverse|slice(0, 1) %}{{ items.name }}{% endfor %}",
  "chartbeat_section"     : [{% if sections %}{% for sectionzs in sections %}"{{ sectionzs.name }}"{% if loop.last == false %}, {% endif %}{% endfor %}{% endif %}],
  {% if section_parent %}
     "page_subsection"       : "{% for items in sections|slice(0, 1) %}{{ items.name }}{% endfor %}",
  {% endif %}
  "page_type"            : "{% if contentsubtype == 'VIDEO' %}video page{% elseif contentsubtype == 'GALLERY' %}gallery page{% else %}article page{% endif %}",
  "tealium_event"        : "{% if contentsubtype == 'VIDEO' %}additional_video_view{% elseif contentsubtype == 'GALLERY' %}additional_gallery_view{% else %}additional_article_view{% endif %}",
  "user_application"     : "{{ every_device }}",
  /*
    "google_value":googleid ,
  "facebook_id" : /_fbp=(fb\.1\.\d+\.\d+)/.exec(window.document.cookie)[1],
  "tealium_visitorid":tealium_visitor_id,
  */
  "eventtimestamp":event_timestamp_utc,
  "NewsKeywords":newsKeywords
 }
 ORA.view({
 "config": {
 "applyMutations": true,
 "sendSessionInfo": true
 },
 "data":utag_data
});


 pSUPERFLY.virtualPage({
      sections:  [{% if sections %}{% for sectionzs in sections %}"{{ sectionzs.name }}"{% if loop.last == false %}, {% endif %}{% endfor %}{% endif %}],
      authors: "{% if authors %}{% for author in authors %}{{ author.name }}{% if loop.last == false %}, {% endif %}{% endfor %}{% elseif author_byline %}{{ author_byline }}{% else %}None{% endif %}",
      path: "{{ replaced_link }}",
      title: "{{ teaser_headline|replace({"\n":'', "\r":''}) }}"
    });

gtag('config', 'G-2VVVMDVFQ8', {
    page_title: "{{ teaser_headline|replace({"\n":'', "\r":''}) }}",
    page_location: "{{ replaced_link }}",
    send_page_view: true 
 });

</script>

