     <div  class="col-lg-12 col-md-12 col-12" data-uuid="{{ uuid }}">
                      <div class="post-title-rows">
                       {% if image %}
                        <div class="thumb-with-image image-zoom"> <a href="{{ permalink }}" title="{{ headline }}">
   {{ render_partial('@base/teasers/partial/image', image|merge({ 'alttext': headline|striptags|replace({'"':'\''}) })) }}
                         </a> </div>
                         {% endif %}
                        <h4> <a href="{{ permalink }}" title="{{ headline }}">
                        
                        {% if contentsubtype=='SPONSOREDCONTENT' && adGroup == 'concept' %}
                              Partner Content: 
                            {% endif %}  {{headline|striptags|replace({'"':'\''}) }} </a> </h4>
                      </div>
                    </div>
                   
 
{% endapply %}
