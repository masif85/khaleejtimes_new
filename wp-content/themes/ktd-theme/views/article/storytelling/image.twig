{% if objectProperties['storytelling'] %}
    {% set storytelling_type = 'data-storytelling="' ~ objectProperties['storytelling'] ~ '"' %}
{% else %}
    {% set storytelling_type = "" %}
{% endif %}

{% if objectProperties['reveal-type'] %}
    {% set reveal_type = 'data-reveal-type="' ~ objectProperties['reveal-type'] ~ '"' %}
{% else %}
    {% set reveal_type = "" %}
{% endif %}

{% if objectProperties['presentation'] %}
    {% set presentation = 'data-presentation="' ~ objectProperties['presentation'] ~ '"' %}
{% else %}
    {% set presentation = "" %}
{% endif %}

{% if cropset['16:9'] %}
    <div class="article_image_crop image_crop" data-uuid="{{ uuid }}" style="display:none;">
        <img
            uuid="{{ uuid }}"
            srcset="{{ fitSrcset|replace({'&q=75': '&q=100', '&type=preview': '&type=primary'}) }}"
            src="{{ fitSrc|replace({'&q=75': '&q=100', '&type=preview': '&type=primary'}) }}"
        >
    </div>
{% elseif cropset['4:3'] %}
    <div class="article_image_crop image_crop" data-uuid="{{ uuid }}" style="display:none;">
        <img
            uuid="{{ uuid }}"
            srcset="{{ fitSrcset|replace({'&q=75': '&q=100', '&type=preview': '&type=primary'}) }}"
            src="{{ fitSrc|replace({'&q=75': '&q=100', '&type=preview': '&type=primary'}) }}"
        >
    </div>
{% else %}
    <div class="article_image" data-uuid="{{ uuid }}" {{ storytelling_type|raw }} {{ reveal_type|raw }} {{ presentation|raw }}>
        <img
            uuid="{{ uuid }}"
            srcset="{{ fitSrcset|replace({'&q=75': '&q=100', '&type=preview': '&type=primary'}) }}"
            src="{{ fitSrc|replace({'&q=75': '&q=100', '&type=preview': '&type=primary'}) }}"
        >
    </div>
{% endif %}

