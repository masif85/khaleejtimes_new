{% extends '@base/main.twig' %}

{% block page_head %}
    {% include "@base/page/part/metatags.twig" %}
{% endblock %}

{% block page_content %}
    <main class="container">
    <h1>{{ title }}</h1>
        <ol>
            {% for article in articles %}
                <li><a href="{{ article.permalink }}">{{ article.published }} - {{ article.headline }}</a></li>
            {% endfor %}
        </ol>
    </main>
{% endblock %}
