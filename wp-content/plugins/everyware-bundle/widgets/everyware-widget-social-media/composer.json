{
    "name": "infomaker/everyware-widget-social-media",
    "description": "A widget that shows the static urls you enter as icons in a widget on the site.",
    "version": "0.2.0",
    "homepage": "https://docs.navigaglobal.com/everyware/",
    "license": "GPL-3.0-or-later",
    "type": "everyware-widget",
    "support": {
        "docs": "https://docs.navigaglobal.com/everyware/widgets-and-components/everyware-widget-social-media",
        "source": "https://composer.infomaker.io/#infomaker/everyware-widget-social-media"
    },
    "authors": [
        {
            "name": "Naviga Web Team",
            "homepage": "https://www.navigaglobal.com/web/"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.infomaker.io/"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "infomaker/everyware-project-plugin": ">=0.9.0"
    },
    "require-dev": {
        "johnpbloch/wordpress-core": "^5.5.3",
        "roave/security-advisories": "dev-master",
        "phpspec/phpspec": "^7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Everyware\\Widget\\SocialMedia\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "vendor/johnpbloch/"
        ],
        "psr-4": {
            "Spec\\": "tests/Spec/"
        }
    }
}
