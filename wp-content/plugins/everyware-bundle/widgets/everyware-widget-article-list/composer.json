{
    "name": "infomaker/everyware-widget-article-list",
    "description": "This package contains the files for the `Article List plugin`. An optional plugin for presenting articles from Open Content.",
    "version": "0.5.0",
    "type": "everyware-widget",
    "homepage": "https://navigaglobal.com",
    "license": "GPL-3.0-or-later",
